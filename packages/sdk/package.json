{
  "name": "@efsp/sdk",
  "version": "0.1.0",
  "description": "EFSP TypeScript SDK",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean",
    "test": "vitest run",
    "prepublishOnly": "npm run build",
    "preflight": "vitest run && tsup --version >/dev/null 2>&1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaretphillips1-ui/efsp.git"
  },
  "homepage": "https://github.com/jaretphillips1-ui/efsp#readme",
  "engines": {
    "node": ">=20"
  }
}
